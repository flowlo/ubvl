expr:	O_SUB(term, expr)	# 4 # bnode->reg = gen_sub(bnode);
expr:	O_SUB(imm, expr)	# 2 # bnode->reg = gen_sub(bnode);
expr:	O_SUB(term, imm)	# 2 # bnode->reg = gen_sub(bnode);
expr:	O_MUL(term, expr)	# 4 # bnode->reg = gen_mul(bnode);
